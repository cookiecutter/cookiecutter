{
    "version": "2.0",
    "requires": {
        "cookiecutter": "2.0.0"
    },
    "template": {
        "name": "cookiecutter-pytest-plugin",
        "variables": [
            {
                "name": "license",
                "type": "string",
                "default": "MIT",
                "choices": [
                    "MIT",
                    "BSD3",
                    "GNU-GPL3",
                    "Apache2",
                    "Mozilla2"
                ]
            }
        ]
    }
}
